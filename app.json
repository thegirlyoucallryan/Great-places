{
  "expo": {
    "name": "React Native Places",
    "slug": "rn-places",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash_icon.png",
    "splash": {
      "image": "./assets/splash_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#232323"
    },
    "updates": {
      "fallbackToCacheTimeout": 4000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/places-adaptive.png",
        "backgroundColor": "#232323"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
